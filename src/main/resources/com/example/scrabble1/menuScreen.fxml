<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="menuPane" prefHeight="750.0" prefWidth="750.0" style="-fx-background-color: NAVAJOWHITE;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.scrabble1.MenuScreen">
   <children>
      <ImageView fitHeight="215.0" fitWidth="358.0" layoutX="203.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Scrabble-Logo-2008.png" />
         </image>
      </ImageView>
      <Button fx:id="newGame" layoutX="291.0" layoutY="307.0" mnemonicParsing="false" onAction="#onNewGame" prefHeight="75.0" prefWidth="185.0" style="-fx-background-color: ANTIQUEWHITE; -fx-border-color: BLACK; -fx-border-width: 2;" stylesheets="@style.css" text="New Game">
         <font>
            <Font name="System Font" size="13.0" />
         </font>
      </Button>
      <Button fx:id="instructions" layoutX="292.0" layoutY="443.0" mnemonicParsing="false" onAction="#OnInstructions" prefHeight="75.0" prefWidth="185.0" style="-fx-background-color: ANTIQUEWHITE; -fx-border-color: BLACK; -fx-border-width: 2;" text="Instructions" />
      <Button fx:id="quit" layoutX="291.0" layoutY="569.0" mnemonicParsing="false" onAction="#onQuit" prefHeight="75.0" prefWidth="185.0" style="-fx-background-color: ANTIQUEWHITE; -fx-border-color: BLACK; -fx-border-width: 2;" text="Quit" textFill="RED" />
   </children>
</AnchorPane>
